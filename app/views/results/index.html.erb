<h1>List of Results</h1>

<table border="1">
  <thead>
    <tr>
      <th>Date</th>
      <th>Player 1</th>
      <th>Team 1</th>
      <th>Score</th>
      <th>Player 2</th>
      <th>Team 2</th>
      <th>Score</th>
      <th>Options</th>
    </tr>
  </thead>

  <% @results.each do |result| %>
    <tr>
      <td><%= link_to result.date, result %></td>
      <td><%= result.player1Id %></td>
      <td><%= result.team1 %></td>
      <td><%= result.team1Score %></td>
      <td><%= result.player2Id %></td>
      <td><%= result.team2 %></td>
      <td><%= result.team2Score %></td>
      <td>
        <%= link_to 'Edit', edit_result_path(result.id) %>
        <%= link_to 'Delete', result, :method => 'delete' %>
      </td>
    </tr>
  <% end -%>
</table>

<p><%= link_to 'New Result', new_result_path %></p>
